
CREATE TABLE users (
    uid TEXT NOT NULL PRIMARY KEY,
    first_name TEXT,
    middle_name TEXT,
    last_name TEXT,
    birthdate INT,
    role TEXT,
    email TEXT,
    phone TEXT,
    created_at INT,
    updated_at INT
);

listUsers: SELECT * FROM users WHERE updated_at = ? LIMIT ?;
addUser:
INSERT INTO users
(uid, first_name, middle_name, last_name, birthdate, role, email, phone, created_at, updated_at)
VALUES (?,?,?,?,?,?,?,?,?,?);
